{
    "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "comma-dangle": "off",
        "react/jsx-filename-extension": "off",
        "prettier/prettier": "error"
    }
}
